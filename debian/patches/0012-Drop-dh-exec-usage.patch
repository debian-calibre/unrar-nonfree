From: YOKOTA Hiroshi <yokota.hgml@gmail.com>
Date: Wed, 16 Mar 2022 22:58:54 +0900
Subject: Drop dh-exec usage

Forwarded: not-needed

dh-exec is outdated.
---
 makefile | 7 +++++--
 1 file changed, 5 insertions(+), 2 deletions(-)

diff --git a/makefile b/makefile
index 18829e3..dfe9626 100644
--- a/makefile
+++ b/makefile
@@ -131,7 +131,7 @@ OBJECTS=rar.o strlist.o strfn.o pathfn.o smallfn.o global.o file.o filefn.o filc
 .cpp.o:
 	$(COMPILE) -D$(WHAT) -c $<
 
-all:	unrar
+all:	unrar-nonfree
 
 install:	install-unrar
 
@@ -142,7 +142,10 @@ clean:
 	rm -f $(OBJECTS) $(UNRAR_OBJ) $(LIB_OBJ)
 
 distclean: clean
-	rm -f unrar libunrar.*
+	rm -f unrar unrar-nonfree libunrar.*
+
+unrar-nonfree: unrar
+	cp unrar unrar-nonfree
 
 unrar:	clean $(OBJECTS) $(UNRAR_OBJ)
 	rm -f unrar
